<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases> 
		<typeAlias type="ant.yum.vo.UserVo" alias="uservo"/>
		<typeAlias type="ant.yum.vo.OrderVo" alias="ordervo"/>
		<typeAlias type="ant.yum.vo.PatientVo" alias="patientvo"/>
		<typeAlias type="ant.yum.vo.DiagnosisVo" alias="diagnosisvo"/>
		<typeAlias type="ant.yum.vo.PresClinicVo" alias="presclinicvo"/>
		<typeAlias type="ant.yum.vo.PresDiseaseVo" alias="presdiseasevo"/>
		<typeAlias type="ant.yum.vo.PresMedicineVo" alias="presmedicinevo"/>
		<typeAlias type="ant.yum.vo.DiseaseVo" alias="diseasevo"/>
		<typeAlias type="ant.yum.vo.MedicineVo" alias="medicinevo"/>
		<typeAlias type="ant.yum.vo.ScheduleVo" alias="schedulevo"/>
		<typeAlias type="ant.yum.vo.PresVo" alias="presvo"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/mappers/order.xml" />
		<mapper resource="mybatis/mappers/patient.xml" />
		<mapper resource="mybatis/mappers/diagnosis.xml" />
		<mapper resource="mybatis/mappers/prescription.xml" />
		<mapper resource="mybatis/mappers/main.xml" />
		<mapper resource="mybatis/mappers/disease.xml" />
		<mapper resource="mybatis/mappers/medicine.xml" />
		<mapper resource="mybatis/mappers/schedule.xml" />
		<mapper resource="mybatis/mappers/disease.xml" />
	</mappers>
</configuration>